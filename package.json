{
  "name": "api-sga",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prepare": "husky install",
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "NODE_ENV=develop nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "format-check": "prettier --check \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint-check": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "NODE_ENV=test && jest --config ./test/jest-e2e.json --runInBand",
    "commitlint": "commitlint --edit",
    "commit": "cz",
    "deploy:staging": "pm2 deploy staging --force",
    "typeorm:migrations:up": "ts-node -r tsconfig-paths/register node_modules/typeorm/cli.js migration:run -d config/ormconfig.ts",
    "typeorm:migrations:down": "ts-node -r tsconfig-paths/register node_modules/typeorm/cli.js migration:revert -d config/ormconfig.ts",
    "typeorm:migrations:generate": "ts-node -r tsconfig-paths/register node_modules/typeorm/cli.js migration:generate -d config/ormconfig.ts ./migrations/$npm_config_name",
    "sga:db:drop": "ts-node -r tsconfig-paths/register src/commands/db/drop-database.command.ts",
    "sga:db:seed:country": "ts-node -r tsconfig-paths/register src/commands/country/import-countries.command.ts",
    "sga:test:db:create": "ts-node -r tsconfig-paths/register src/commands/db/create-e2e-database.command.ts",
    "sga:test:db:drop": "NODE_ENV=test ts-node -r tsconfig-paths/register src/commands/db/drop-database.command.ts",
    "typeorm:test:migrations:up": "NODE_ENV=test ts-node -r tsconfig-paths/register node_modules/typeorm/cli.js migration:run -d config/ormconfig.ts",
    "sga:test:db:seed:country": "NODE_ENV=test ts-node -r tsconfig-paths/register src/commands/country/import-countries.command.ts"
  },
  "dependencies": {
    "@nestjs/axios": "^3.0.1",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.0.0",
    "@nestjs/event-emitter": "^2.0.3",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/terminus": "^10.2.0",
    "@nestjs/typeorm": "^10.0.1",
    "bcrypt": "^5.1.1",
    "joi": "^17.11.0",
    "nestjs-pino": "^3.5.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.11.3",
    "pino-http": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "semver": "^7.5.4",
    "typeorm": "^0.3.17",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.2",
    "@commitlint/config-conventional": "^18.4.2",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/passport-jwt": "^3.0.13",
    "@types/passport-local": "^1.0.38",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^15.1.0",
    "pino-pretty": "^10.2.3",
    "pm2": "^5.3.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^#admin-user/(.*)$": "<rootDir>/sga/admin-user/$1",
      "^#shared/(.*)$": "<rootDir>/shared/shared/$1",
      "^#business-unit/(.*)$": "<rootDir>/sga/business-unit/$1",
      "^#commands/(.*)$": "<rootDir>/commands/$1",
      "^#config/(.*)$": "<rootDir>/../config/$1",
      "^#test/(.*)$": "<rootDir>/../test/$1",
      "^#/(.*)$": "<rootDir>/$1",
      "^&/(.*)$": "<rootDir>/../$1"
    }
  }
}
