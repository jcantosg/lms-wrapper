{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "^#admin-user/(.*)$": "<rootDir>/../src/sga/admin-user/$1",
    "^#shared/(.*)$": "<rootDir>/../src/shared/shared/$1",
    "^#business-unit/(.*)$": "<rootDir>/../src/sga/business-unit/$1",
    "^#commands/(.*)$": "<rootDir>/../src/commands/$1",
    "^#config/(.*)$": "<rootDir>/../config/$1",
    "^#test/(.*)$": "<rootDir>/../test/$1",
    "^#/(.*)$": "<rootDir>/../src/$1"
  }
}